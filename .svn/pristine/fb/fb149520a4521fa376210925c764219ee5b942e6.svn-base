public class Central {
    private String Name;
    private int imageDrawable;

    public Central(String name, int imageDrawable) {
        Name = name;
        this.imageDrawable = imageDrawable;
    }

    public String getName() {
        return Name;
    }

    public int getImageDrawable() {
        return imageDrawable;
    }
}
