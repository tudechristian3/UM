package com.ultramega.shop.pojo;


public class OrderSkus {
    private String itemid;
    private String catclass;
    private int quantity;
    private String packageid;
    private String itempicurl;

    public OrderSkus(String itemid, String catclass, int quantity, String packageid, String itempicurl) {
        this.itemid = itemid;
        this.catclass = catclass;
        this.quantity = quantity;
        this.packageid = packageid;
        this.itempicurl = itempicurl;
    }

    public String getItemid() {
        return itemid;
    }

    public void setItemid(String itemid) {
        this.itemid = itemid;
    }

    public String getCatclass() {
        return catclass;
    }

    public void setCatclass(String catclass) {
        this.catclass = catclass;
    }

    public int getQuantity() {
        return quantity;
    }

    public void setQuantity(int quantity) {
        this.quantity = quantity;
    }

    public String getPackageid() {
        return packageid;
    }

    public void setPackageid(String packageid) {
        this.packageid = packageid;
    }

    public String getItempicurl() {
        return itempicurl;
    }

    public void setItempicurl(String itempicurl) {
        this.itempicurl = itempicurl;
    }
}
